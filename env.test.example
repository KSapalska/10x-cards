# Playwright E2E Testing Environment Variables
# Skopiuj ten plik jako .env.test i wypełnij właściwymi wartościami

# Base URL for Playwright tests
PLAYWRIGHT_BASE_URL=http://localhost:4321

# Test user credentials (create this user in your test database)
# Musisz stworzyć tego użytkownika ręcznie w bazie testowej
E2E_USERNAME=test@example.com
E2E_PASSWORD=testpassword123

# Supabase configuration (pointing to test database or project)
# Użyj osobnego projektu Supabase dla testów lub tego samego co development
PUBLIC_SUPABASE_URL=your_test_supabase_url
PUBLIC_SUPABASE_ANON_KEY=your_test_supabase_anon_key


